{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "next/core-web-vitals","next/typescript"
    ],
    "env": {
      "node": true,
      "es2020": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
     
      "no-console": "warn",  
      "@typescript-eslint/no-require-imports": "off" 
  }
}